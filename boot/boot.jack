class boot {
	
	function void boot() {
		
		//doing this init first cause its required to be activated before printString()
		do Memory.init();
		do Screen.init();
		do Output.init();
		do Output.printString("Booting into JackOS...");
		do Math.init();
		do Keyboard.init();
		do Disk.init();
		do Sys.init();
		do kernel.kernel();
	
	}
}
